depend() {
	need hcid
}

start() {
	ebegin "Starting sdpd"
	start-stop-daemon --start --quiet --exec /usr/sbin/sdpd
	eend $?
}

stop() {
	ebegin "Stopping sdpd"
	start-stop-daemon --stop --quiet --exec /usr/sbin/sdpd
	eend $?
}

